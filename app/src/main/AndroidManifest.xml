<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yoldash">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddVocab"></activity>
        <activity android:name=".EditProfile"></activity>
        <activity android:name=".VocabList"></activity>
        <activity android:name=".Information"></activity>
        <activity android:name=".BoxActivity"></activity>
        <activity android:name=".EasyBox"></activity>
        <activity android:name=".HardBox"></activity>
        <activity android:name=".MiddleBox"></activity>
        <activity android:name=".SearchActivity"></activity>
    </application>

</manifest>